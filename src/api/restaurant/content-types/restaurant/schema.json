{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": { "type": "string", "required": true },
    "cuisine": { "type": "string" },
    "rating": { "type": "decimal" },
    "priceRange": { "type": "enumeration", "enum": ["budget", "moderate", "expensive", "luxury"], "default": "moderate" },
    "image": { "type": "media", "multiple": false, "allowedTypes": ["images"] },
    "location": { "type": "string" },
    "latitude": { "type": "decimal" },
    "longitude": { "type": "decimal" },
    "description": { "type": "text" },
    "features": { "type": "json" },
    "discounts": { "type": "json" },
    "maxDailyBookings": { "type": "integer", "default": 50 },
    "currentDailyBookings": { "type": "integer", "default": 0 },
    "isAllYouCanEat": { "type": "boolean", "default": false },
    "topDiscount": { "type": "integer", "default": 0 },
    "relevanceScore": { "type": "integer", "default": 0 },
    "contact": { "type": "string" },
    "openHours": { "type": "string" }
    ,
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "restaurant"
    }
  }
}


