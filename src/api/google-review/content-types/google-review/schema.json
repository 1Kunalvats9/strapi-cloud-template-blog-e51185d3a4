{
  "kind": "collectionType",
  "collectionName": "google_reviews",
  "info": {
    "singularName": "google-review",
    "pluralName": "google-reviews",
    "displayName": "Google Review",
    "description": "Multi-platform business reviews from Google, Yelp, Foursquare, and Facebook"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "source_platform": {
      "type": "enumeration",
      "enum": [
        "Google",
        "Facebook",
        "Yelp",
        "Foursquare"
      ],
      "required": true
    },
    "source_review_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "AuthorName": {
      "type": "string",
      "required": true
    },
    "AuthorProfilePhotoUrl": {
      "type": "string"
    },
    "AuthorProfileUrl": {
      "type": "string"
    },
    "Rating": {
      "type": "integer",
      "min": 1,
      "max": 5,
      "required": true
    },
    "ReviewText": {
      "type": "text",
      "required": true
    },
    "ReviewTime": {
      "type": "datetime",
      "required": true
    },
    "RelativeTimeDescription": {
      "type": "string"
    },
    "BusinessName": {
      "type": "string",
      "required": true
    },
    "BusinessAddress": {
      "type": "string"
    },
    "BusinessType": {
      "type": "string"
    },
    "BusinessUrl": {
      "type": "string"
    },
    "business": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::business.business",
      "inversedBy": "google_reviews"
    },
    "cache_expiry_date": {
      "type": "datetime",
      "required": true
    },
    "Verified": {
      "type": "boolean",
      "default": false
    },
    "IsActive": {
      "type": "boolean",
      "default": true
    },
    "Featured": {
      "type": "boolean",
      "default": false
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Restaurant",
        "Hotel",
        "Spa",
        "Shopping",
        "Entertainment",
        "Nightlife",
        "Tourism",
        "Transportation",
        "Health",
        "Other"
      ]
    },
    "Location": {
      "type": "string"
    },
    "Language": {
      "type": "string",
      "default": "en"
    },
    "LastUpdated": {
      "type": "datetime"
    }
  }
}
